' IMG
!function $TraceIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure Trace(e_alias, e_label)
    GCEntity(e_alias, e_label, Trace, Trace)
!endprocedure

!procedure Trace(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, Trace, Trace)
!endprocedure

$GCDefineEntityStyle(Trace)


' Participant
!procedure TraceParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, Trace, Trace)
!endprocedure

!procedure TraceParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, Trace, Trace)
!endprocedure

$GCDefineParticipantStyle(Trace)


' Group
!procedure TraceGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Trace, TraceGroup)
!endprocedure

!procedure TraceGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Trace, TraceGroup)
!endprocedure

$GCDefineGroupStyle(TraceGroup)
