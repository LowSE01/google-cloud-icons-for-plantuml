' IMG
!function $ApiIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure Api(e_alias, e_label)
    GCEntity(e_alias, e_label, Api, Api)
!endprocedure

!procedure Api(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, Api, Api)
!endprocedure

$GCDefineEntityStyle(Api)


' Participant
!procedure ApiParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, Api, Api)
!endprocedure

!procedure ApiParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, Api, Api)
!endprocedure

$GCDefineParticipantStyle(Api)


' Group
!procedure ApiGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Api, ApiGroup)
!endprocedure

!procedure ApiGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Api, ApiGroup)
!endprocedure

$GCDefineGroupStyle(ApiGroup)
