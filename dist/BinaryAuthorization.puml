!endprocedure

!procedure BinaryAuthorizationParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, BinaryAuthorization, BinaryAuthorization)
!endprocedure

$GCDefineParticipantStyle(BinaryAuthorization)


