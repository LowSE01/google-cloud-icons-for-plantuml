!endfunction


' Entity
!procedure StandardNetworkTier(e_alias, e_label)
    GCEntity(e_alias, e_label, StandardNetworkTier, StandardNetworkTier)
!endprocedure

!procedure StandardNetworkTier(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, StandardNetworkTier, StandardNetworkTier)
!endprocedure

$GCDefineEntityStyle(StandardNetworkTier)


' Participant
!procedure StandardNetworkTierParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, StandardNetworkTier, StandardNetworkTier)
!endprocedure

!procedure StandardNetworkTierParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, StandardNetworkTier, StandardNetworkTier)
!endprocedure

$GCDefineParticipantStyle(StandardNetworkTier)


' Group
!procedure StandardNetworkTierGroup(g_alias, g_label="")
