' IMG
!function $CertificateManagerIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure CertificateManager(e_alias, e_label)
    GCEntity(e_alias, e_label, CertificateManager, CertificateManager)
!endprocedure

!procedure CertificateManager(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, CertificateManager, CertificateManager)
!endprocedure

$GCDefineEntityStyle(CertificateManager)


' Participant
!procedure CertificateManagerParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, CertificateManager, CertificateManager)
!endprocedure

!procedure CertificateManagerParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, CertificateManager, CertificateManager)
!endprocedure

$GCDefineParticipantStyle(CertificateManager)


' Group
!procedure CertificateManagerGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, CertificateManager, CertificateManagerGroup)
!endprocedure

!procedure CertificateManagerGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, CertificateManager, CertificateManagerGroup)
!endprocedure

$GCDefineGroupStyle(CertificateManagerGroup)
