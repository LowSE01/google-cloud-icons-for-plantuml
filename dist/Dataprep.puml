!function $DataprepIMG($scale)
    !return "<img data:image/svg+xml;base64,{scale="+$scale+"}>"
!endfunction


!procedure Dataprep(e_alias, e_label)
    GCEntity(e_alias, e_label, Dataprep, Dataprep)
!endprocedure

!procedure Dataprep(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, Dataprep, Dataprep)
!endprocedure

$GCDefineEntityStyle(Dataprep)


!procedure DataprepParticipant(p_alias, p_label)
