!endprocedure

!procedure DataprepParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, Dataprep, Dataprep)
!endprocedure


!procedure DataprepGroup(g_alias, g_label="")
