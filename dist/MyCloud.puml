' IMG
!function $MyCloudIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure MyCloud(e_alias, e_label)
    GCEntity(e_alias, e_label, MyCloud, MyCloud)
!endprocedure

!procedure MyCloud(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, MyCloud, MyCloud)
!endprocedure

$GCDefineEntityStyle(MyCloud)


' Participant
!procedure MyCloudParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, MyCloud, MyCloud)
!endprocedure

!procedure MyCloudParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, MyCloud, MyCloud)
!endprocedure

$GCDefineParticipantStyle(MyCloud)


' Group
!procedure MyCloudGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, MyCloud, MyCloudGroup)
!endprocedure

!procedure MyCloudGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, MyCloud, MyCloudGroup)
!endprocedure

$GCDefineGroupStyle(MyCloudGroup)
