' IMG
!function $QuotasIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure Quotas(e_alias, e_label)
    GCEntity(e_alias, e_label, Quotas, Quotas)
!endprocedure

!procedure Quotas(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, Quotas, Quotas)
!endprocedure

$GCDefineEntityStyle(Quotas)


' Participant
!procedure QuotasParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, Quotas, Quotas)
!endprocedure

!procedure QuotasParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, Quotas, Quotas)
!endprocedure

$GCDefineParticipantStyle(Quotas)


' Group
!procedure QuotasGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Quotas, QuotasGroup)
!endprocedure

!procedure QuotasGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Quotas, QuotasGroup)
!endprocedure

$GCDefineGroupStyle(QuotasGroup)
