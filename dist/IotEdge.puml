!endprocedure

$GCDefineEntityStyle(IotEdge)


' Participant
!procedure IotEdgeParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, IotEdge, IotEdge)
!endprocedure

!procedure IotEdgeParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, IotEdge, IotEdge)
!endprocedure

$GCDefineParticipantStyle(IotEdge)


' Group
!procedure IotEdgeGroup(g_alias, g_label="")
