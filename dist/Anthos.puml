!function $AnthosIMG($scale)
    !return "<img data:image/svg+xml;base64,{scale="+$scale+"}>"
!endfunction


!procedure Anthos(e_alias, e_label)
    GCEntity(e_alias, e_label, Anthos, Anthos)
!endprocedure

!procedure Anthos(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, Anthos, Anthos)
!endprocedure

$GCDefineEntityStyle(Anthos)


!procedure AnthosParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, Anthos, Anthos)
!endprocedure

!procedure AnthosParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, Anthos, Anthos)
!endprocedure


!procedure AnthosGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Anthos, AnthosGroup)
!endprocedure

!procedure AnthosGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Anthos, AnthosGroup)
!endprocedure

$GCDefineGroupStyle(AnthosGroup)
