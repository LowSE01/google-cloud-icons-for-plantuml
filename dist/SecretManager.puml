' IMG
!function $SecretManagerIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure SecretManager(e_alias, e_label)
    GCEntity(e_alias, e_label, SecretManager, SecretManager)
!endprocedure

!procedure SecretManager(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, SecretManager, SecretManager)
!endprocedure

$GCDefineEntityStyle(SecretManager)


' Participant
!procedure SecretManagerParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, SecretManager, SecretManager)
!endprocedure

!procedure SecretManagerParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, SecretManager, SecretManager)
!endprocedure

$GCDefineParticipantStyle(SecretManager)


' Group
!procedure SecretManagerGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, SecretManager, SecretManagerGroup)
!endprocedure

!procedure SecretManagerGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, SecretManager, SecretManagerGroup)
!endprocedure

$GCDefineGroupStyle(SecretManagerGroup)
