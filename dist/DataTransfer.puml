' IMG
!function $DataTransferIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure DataTransfer(e_alias, e_label)
    GCEntity(e_alias, e_label, DataTransfer, DataTransfer)
!endprocedure

!procedure DataTransfer(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, DataTransfer, DataTransfer)
!endprocedure

$GCDefineEntityStyle(DataTransfer)


' Participant
!procedure DataTransferParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, DataTransfer, DataTransfer)
!endprocedure

!procedure DataTransferParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, DataTransfer, DataTransfer)
!endprocedure

$GCDefineParticipantStyle(DataTransfer)


' Group
!procedure DataTransferGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, DataTransfer, DataTransferGroup)
!endprocedure

!procedure DataTransferGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, DataTransfer, DataTransferGroup)
!endprocedure

$GCDefineGroupStyle(DataTransferGroup)
