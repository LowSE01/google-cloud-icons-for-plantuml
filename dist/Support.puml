' IMG
!function $SupportIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure Support(e_alias, e_label)
    GCEntity(e_alias, e_label, Support, Support)
!endprocedure

!procedure Support(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, Support, Support)
!endprocedure

$GCDefineEntityStyle(Support)


' Participant
!procedure SupportParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, Support, Support)
!endprocedure

!procedure SupportParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, Support, Support)
!endprocedure

$GCDefineParticipantStyle(Support)


' Group
!procedure SupportGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Support, SupportGroup)
!endprocedure

!procedure SupportGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Support, SupportGroup)
!endprocedure

$GCDefineGroupStyle(SupportGroup)
