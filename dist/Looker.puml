!function $LookerIMG($scale)
    !return "<img data:image/svg+xml;base64,{scale="+$scale+"}>"
!endfunction


!procedure Looker(e_alias, e_label)
    GCEntity(e_alias, e_label, Looker, Looker)
!endprocedure

!procedure Looker(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, Looker, Looker)
!endprocedure

$GCDefineEntityStyle(Looker)


!procedure LookerParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, Looker, Looker)
!endprocedure

!procedure LookerParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, Looker, Looker)
!endprocedure


!procedure LookerGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Looker, LookerGroup)
!endprocedure

!procedure LookerGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Looker, LookerGroup)
!endprocedure

$GCDefineGroupStyle(LookerGroup)
