' IMG
!function $AdministrationIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure Administration(e_alias, e_label)
    GCEntity(e_alias, e_label, Administration, Administration)
!endprocedure

!procedure Administration(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, Administration, Administration)
!endprocedure

$GCDefineEntityStyle(Administration)


' Participant
!procedure AdministrationParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, Administration, Administration)
!endprocedure

!procedure AdministrationParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, Administration, Administration)
!endprocedure

$GCDefineParticipantStyle(Administration)


' Group
!procedure AdministrationGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Administration, AdministrationGroup)
!endprocedure

!procedure AdministrationGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Administration, AdministrationGroup)
!endprocedure

$GCDefineGroupStyle(AdministrationGroup)
