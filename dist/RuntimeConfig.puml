' IMG
!function $RuntimeConfigIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure RuntimeConfig(e_alias, e_label)
    GCEntity(e_alias, e_label, RuntimeConfig, RuntimeConfig)
!endprocedure

!procedure RuntimeConfig(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, RuntimeConfig, RuntimeConfig)
!endprocedure

$GCDefineEntityStyle(RuntimeConfig)


' Participant
!procedure RuntimeConfigParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, RuntimeConfig, RuntimeConfig)
!endprocedure

!procedure RuntimeConfigParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, RuntimeConfig, RuntimeConfig)
!endprocedure

$GCDefineParticipantStyle(RuntimeConfig)


' Group
!procedure RuntimeConfigGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, RuntimeConfig, RuntimeConfigGroup)
!endprocedure

!procedure RuntimeConfigGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, RuntimeConfig, RuntimeConfigGroup)
!endprocedure

$GCDefineGroupStyle(RuntimeConfigGroup)
