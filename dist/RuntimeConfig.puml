!endprocedure

!procedure RuntimeConfigParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, RuntimeConfig, RuntimeConfig)
!endprocedure


!procedure RuntimeConfigGroup(g_alias, g_label="")
