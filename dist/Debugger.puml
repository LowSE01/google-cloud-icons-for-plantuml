' IMG
!function $DebuggerIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure Debugger(e_alias, e_label)
    GCEntity(e_alias, e_label, Debugger, Debugger)
!endprocedure

!procedure Debugger(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, Debugger, Debugger)
!endprocedure

$GCDefineEntityStyle(Debugger)


' Participant
!procedure DebuggerParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, Debugger, Debugger)
!endprocedure

!procedure DebuggerParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, Debugger, Debugger)
!endprocedure

$GCDefineParticipantStyle(Debugger)


' Group
!procedure DebuggerGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Debugger, DebuggerGroup)
!endprocedure

!procedure DebuggerGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Debugger, DebuggerGroup)
!endprocedure

$GCDefineGroupStyle(DebuggerGroup)
