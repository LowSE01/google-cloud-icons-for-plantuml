!function $CatalogIMG($scale)
    !return "<img data:image/svg+xml;base64,{scale="+$scale+"}>"
!endfunction


!procedure Catalog(e_alias, e_label)
    GCEntity(e_alias, e_label, Catalog, Catalog)
!endprocedure

!procedure Catalog(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, Catalog, Catalog)
!endprocedure

$GCDefineEntityStyle(Catalog)


!procedure CatalogParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, Catalog, Catalog)
!endprocedure

!procedure CatalogParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, Catalog, Catalog)
!endprocedure


!procedure CatalogGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Catalog, CatalogGroup)
!endprocedure

!procedure CatalogGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Catalog, CatalogGroup)
!endprocedure

$GCDefineGroupStyle(CatalogGroup)
