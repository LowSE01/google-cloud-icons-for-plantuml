!endprocedure

$GCDefineEntityStyle(ComputeEngine)


' Participant
!procedure ComputeEngineParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, ComputeEngine, ComputeEngine)
!endprocedure

!procedure ComputeEngineParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, ComputeEngine, ComputeEngine)
!endprocedure

$GCDefineParticipantStyle(ComputeEngine)


' Group
!procedure ComputeEngineGroup(g_alias, g_label="")
