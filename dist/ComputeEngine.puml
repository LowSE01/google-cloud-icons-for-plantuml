!endfunction


' Entity
!procedure ComputeEngine(e_alias, e_label)
    GCEntity(e_alias, e_label, ComputeEngine, ComputeEngine)
!endprocedure

!procedure ComputeEngine(e_alias, e_label, e_descr)
