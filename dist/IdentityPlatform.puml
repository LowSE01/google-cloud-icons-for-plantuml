' IMG
!function $IdentityPlatformIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure IdentityPlatform(e_alias, e_label)
    GCEntity(e_alias, e_label, IdentityPlatform, IdentityPlatform)
!endprocedure

!procedure IdentityPlatform(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, IdentityPlatform, IdentityPlatform)
!endprocedure

$GCDefineEntityStyle(IdentityPlatform)


' Participant
!procedure IdentityPlatformParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, IdentityPlatform, IdentityPlatform)
!endprocedure

!procedure IdentityPlatformParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, IdentityPlatform, IdentityPlatform)
!endprocedure

$GCDefineParticipantStyle(IdentityPlatform)


' Group
!procedure IdentityPlatformGroup(g_alias, g_label="")
