' IMG
!function $VisualInspectionIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure VisualInspection(e_alias, e_label)
    GCEntity(e_alias, e_label, VisualInspection, VisualInspection)
!endprocedure

!procedure VisualInspection(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, VisualInspection, VisualInspection)
!endprocedure

$GCDefineEntityStyle(VisualInspection)


' Participant
!procedure VisualInspectionParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, VisualInspection, VisualInspection)
!endprocedure

!procedure VisualInspectionParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, VisualInspection, VisualInspection)
!endprocedure

$GCDefineParticipantStyle(VisualInspection)


' Group
!procedure VisualInspectionGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, VisualInspection, VisualInspectionGroup)
!endprocedure

!procedure VisualInspectionGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, VisualInspection, VisualInspectionGroup)
!endprocedure

$GCDefineGroupStyle(VisualInspectionGroup)
