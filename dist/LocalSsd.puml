' IMG
!function $LocalSsdIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure LocalSsd(e_alias, e_label)
    GCEntity(e_alias, e_label, LocalSsd, LocalSsd)
!endprocedure

!procedure LocalSsd(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, LocalSsd, LocalSsd)
!endprocedure

$GCDefineEntityStyle(LocalSsd)


' Participant
!procedure LocalSsdParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, LocalSsd, LocalSsd)
!endprocedure

!procedure LocalSsdParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, LocalSsd, LocalSsd)
!endprocedure

$GCDefineParticipantStyle(LocalSsd)


' Group
!procedure LocalSsdGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, LocalSsd, LocalSsdGroup)
!endprocedure

!procedure LocalSsdGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, LocalSsd, LocalSsdGroup)
!endprocedure

$GCDefineGroupStyle(LocalSsdGroup)
