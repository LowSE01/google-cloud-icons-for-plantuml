' IMG
!function $CloudGenericIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure CloudGeneric(e_alias, e_label)
    GCEntity(e_alias, e_label, CloudGeneric, CloudGeneric)
!endprocedure

!procedure CloudGeneric(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, CloudGeneric, CloudGeneric)
!endprocedure

$GCDefineEntityStyle(CloudGeneric)


' Participant
!procedure CloudGenericParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, CloudGeneric, CloudGeneric)
!endprocedure

!procedure CloudGenericParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, CloudGeneric, CloudGeneric)
!endprocedure

$GCDefineParticipantStyle(CloudGeneric)


' Group
!procedure CloudGenericGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, CloudGeneric, CloudGenericGroup)
!endprocedure

!procedure CloudGenericGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, CloudGeneric, CloudGenericGroup)
!endprocedure

$GCDefineGroupStyle(CloudGenericGroup)
