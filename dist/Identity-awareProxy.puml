!endprocedure

!procedure Identity-awareProxyParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, Identity-awareProxy, Identity-awareProxy)
!endprocedure


!procedure Identity-awareProxyGroup(g_alias, g_label="")
