' IMG
!function $CloudApisIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure CloudApis(e_alias, e_label)
    GCEntity(e_alias, e_label, CloudApis, CloudApis)
!endprocedure

!procedure CloudApis(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, CloudApis, CloudApis)
!endprocedure

$GCDefineEntityStyle(CloudApis)


' Participant
!procedure CloudApisParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, CloudApis, CloudApis)
!endprocedure

!procedure CloudApisParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, CloudApis, CloudApis)
!endprocedure

$GCDefineParticipantStyle(CloudApis)


' Group
!procedure CloudApisGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, CloudApis, CloudApisGroup)
!endprocedure

!procedure CloudApisGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, CloudApis, CloudApisGroup)
!endprocedure

$GCDefineGroupStyle(CloudApisGroup)
