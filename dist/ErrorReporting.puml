' IMG
!function $ErrorReportingIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure ErrorReporting(e_alias, e_label)
    GCEntity(e_alias, e_label, ErrorReporting, ErrorReporting)
!endprocedure

!procedure ErrorReporting(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, ErrorReporting, ErrorReporting)
!endprocedure

$GCDefineEntityStyle(ErrorReporting)


' Participant
!procedure ErrorReportingParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, ErrorReporting, ErrorReporting)
!endprocedure

!procedure ErrorReportingParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, ErrorReporting, ErrorReporting)
!endprocedure

$GCDefineParticipantStyle(ErrorReporting)


' Group
!procedure ErrorReportingGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, ErrorReporting, ErrorReportingGroup)
!endprocedure

!procedure ErrorReportingGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, ErrorReporting, ErrorReportingGroup)
!endprocedure

$GCDefineGroupStyle(ErrorReportingGroup)
