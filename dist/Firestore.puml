' IMG
!function $FirestoreIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure Firestore(e_alias, e_label)
    GCEntity(e_alias, e_label, Firestore, Firestore)
!endprocedure

!procedure Firestore(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, Firestore, Firestore)
!endprocedure

$GCDefineEntityStyle(Firestore)


' Participant
!procedure FirestoreParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, Firestore, Firestore)
!endprocedure

!procedure FirestoreParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, Firestore, Firestore)
!endprocedure

$GCDefineParticipantStyle(Firestore)


' Group
!procedure FirestoreGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Firestore, FirestoreGroup)
!endprocedure

!procedure FirestoreGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Firestore, FirestoreGroup)
!endprocedure

$GCDefineGroupStyle(FirestoreGroup)
