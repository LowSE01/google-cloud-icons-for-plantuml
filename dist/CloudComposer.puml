' IMG
!function $CloudComposerIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure CloudComposer(e_alias, e_label)
    GCEntity(e_alias, e_label, CloudComposer, CloudComposer)
!endprocedure

!procedure CloudComposer(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, CloudComposer, CloudComposer)
!endprocedure

$GCDefineEntityStyle(CloudComposer)


' Participant
!procedure CloudComposerParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, CloudComposer, CloudComposer)
!endprocedure

!procedure CloudComposerParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, CloudComposer, CloudComposer)
!endprocedure

$GCDefineParticipantStyle(CloudComposer)


' Group
!procedure CloudComposerGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, CloudComposer, CloudComposerGroup)
!endprocedure

!procedure CloudComposerGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, CloudComposer, CloudComposerGroup)
!endprocedure

$GCDefineGroupStyle(CloudComposerGroup)
