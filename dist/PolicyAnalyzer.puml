' IMG
!function $PolicyAnalyzerIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure PolicyAnalyzer(e_alias, e_label)
    GCEntity(e_alias, e_label, PolicyAnalyzer, PolicyAnalyzer)
!endprocedure

!procedure PolicyAnalyzer(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, PolicyAnalyzer, PolicyAnalyzer)
!endprocedure

$GCDefineEntityStyle(PolicyAnalyzer)


' Participant
!procedure PolicyAnalyzerParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, PolicyAnalyzer, PolicyAnalyzer)
!endprocedure

!procedure PolicyAnalyzerParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, PolicyAnalyzer, PolicyAnalyzer)
!endprocedure

$GCDefineParticipantStyle(PolicyAnalyzer)


' Group
!procedure PolicyAnalyzerGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, PolicyAnalyzer, PolicyAnalyzerGroup)
!endprocedure

!procedure PolicyAnalyzerGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, PolicyAnalyzer, PolicyAnalyzerGroup)
!endprocedure

$GCDefineGroupStyle(PolicyAnalyzerGroup)
