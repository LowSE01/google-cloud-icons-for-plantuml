' IMG
!function $TransferIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure Transfer(e_alias, e_label)
    GCEntity(e_alias, e_label, Transfer, Transfer)
!endprocedure

!procedure Transfer(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, Transfer, Transfer)
!endprocedure

$GCDefineEntityStyle(Transfer)


' Participant
!procedure TransferParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, Transfer, Transfer)
!endprocedure

!procedure TransferParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, Transfer, Transfer)
!endprocedure

$GCDefineParticipantStyle(Transfer)


' Group
!procedure TransferGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Transfer, TransferGroup)
!endprocedure

!procedure TransferGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Transfer, TransferGroup)
!endprocedure

$GCDefineGroupStyle(TransferGroup)
