' IMG
!function $DatastoreIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure Datastore(e_alias, e_label)
    GCEntity(e_alias, e_label, Datastore, Datastore)
!endprocedure

!procedure Datastore(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, Datastore, Datastore)
!endprocedure

$GCDefineEntityStyle(Datastore)


' Participant
!procedure DatastoreParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, Datastore, Datastore)
!endprocedure

!procedure DatastoreParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, Datastore, Datastore)
!endprocedure

$GCDefineParticipantStyle(Datastore)


' Group
!procedure DatastoreGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Datastore, DatastoreGroup)
!endprocedure

!procedure DatastoreGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Datastore, DatastoreGroup)
!endprocedure

$GCDefineGroupStyle(DatastoreGroup)
