' IMG
!function $OnboardingIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure Onboarding(e_alias, e_label)
    GCEntity(e_alias, e_label, Onboarding, Onboarding)
!endprocedure

!procedure Onboarding(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, Onboarding, Onboarding)
!endprocedure

$GCDefineEntityStyle(Onboarding)


' Participant
!procedure OnboardingParticipant(p_alias, p_label)
    GCParticipant(p_alias, p_label, Onboarding, Onboarding)
!endprocedure

!procedure OnboardingParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, Onboarding, Onboarding)
!endprocedure

$GCDefineParticipantStyle(Onboarding)


' Group
!procedure OnboardingGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Onboarding, OnboardingGroup)
!endprocedure

!procedure OnboardingGroup(g_alias, g_label="")
    $GCDefineGroup(g_alias, g_label, Onboarding, OnboardingGroup)
!endprocedure

$GCDefineGroupStyle(OnboardingGroup)
