!endprocedure

!procedure ReleaseNotesParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, ReleaseNotes, ReleaseNotes)
!endprocedure

$GCDefineParticipantStyle(ReleaseNotes)


