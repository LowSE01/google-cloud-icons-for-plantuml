!endprocedure

!procedure FilestoreParticipant(p_alias, p_label, p_descr)
    GCParticipant(p_alias, p_label, p_descr, Filestore, Filestore)
!endprocedure

$GCDefineParticipantStyle(Filestore)


' Group
!procedure FilestoreGroup(g_alias, g_label="")
