' IMG
!function $FilestoreIMG($scale)
    !return "<img data:image/png;base64,{scale="+$scale+"}>"
!endfunction


' Entity
!procedure Filestore(e_alias, e_label)
    GCEntity(e_alias, e_label, Filestore, Filestore)
!endprocedure

!procedure Filestore(e_alias, e_label, e_descr)
    GCEntity(e_alias, e_label, e_descr, Filestore, Filestore)
!endprocedure

$GCDefineEntityStyle(Filestore)


' Participant
!procedure FilestoreParticipant(p_alias, p_label)
